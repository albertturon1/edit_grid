{
  "type": "module",
  "scripts": {
    "dev": "vite --port=3001",
    "build": "vite build",
    "serve": "vite preview",
    "start": "vite",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "tsc": "tsc --noEmit",
    "unit": "vitest run",
    "unit:watch": "vitest",
    "e2e": "playwright test",
    "party:dev": "partykit dev",
    "party:deploy": "partykit deploy",
    "prepare": "husky install",
    "feedback": "bun fmt && bun lint && bun tsc && bun unit --passWithNoTests && bun e2e"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-router": "^1.145.7",
    "@tanstack/react-start": "^1.157.16",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "idb-keyval": "^6.2.2",
    "lucide-react": "^0.563.0",
    "nitro": "npm:nitro-nightly@latest",
    "papaparse": "^5.5.3",
    "partykit": "^0.0.115",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.70.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.1",
    "y-partykit": "^0.0.33",
    "yjs": "^13.6.29",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/react-devtools": "^0.9.3",
    "@tanstack/react-router-devtools": "^1.157.16",
    "@tanstack/router-devtools": "^1.145.7",
    "@tanstack/router-plugin": "^1.145.7",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.19.3",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "happy-dom": "^20.3.9",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.39.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "bun fmt:check",
      "bun lint",
      "sh -c \"bun tsc --noEmit -p tsconfig.json\"",
      "bun unit --passWithNoTests",
      "bun e2e --pass-with-no-tests"
    ]
  },
  "packageManager": "bun@1.3.5"
}
